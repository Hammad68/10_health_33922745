<!DOCTYPE html>
<html>
<head>
  <title>Online Registered Users</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }
    th, td {
      padding: 8px 12px;
      border: 1px solid #ccc;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>

  <h1>Online Registered Users</h1>

  <% if (onlineAccess && onlineAccess.length > 0) { %>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Patient ID</th>
          <th>Username</th>
          <th>Name</th>
          <th>Role</th>
        </tr>
      </thead>
      <tbody>
        <% onlineAccess.forEach(user => { %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.patient_id %></td>
            <td><%= user.username %></td>
            <td><%= user.name %></td>
            <td><%= user.role %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  <% } else { %>
    <p>No online registered users found.</p>
  <% } %>

</body>
</html>